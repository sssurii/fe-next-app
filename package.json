{
  "name": "fl-web-boilerplate",
  "version": "0.8.0",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-fix": "next lint  --fix",
    "test": "CI=true jest --env=jsdom",
    "test:watch": "jest --env=jsdom --watch",
    "test:coverage": "jest --env=jsdom --watchAll --coverage --collectCoverageFrom='[\"src/**/*.{ts,tsx}\", \"!src/**/*.stories.tsx\"]'",
    "cypress:run": "npx cypress run --headless",
    "cypress:run:electron": "npx cypress run -b electron --headless",
    "cypress:open": "npx cypress open",
    "cypress:open:uat": "export CYPRESS_BASE_URL=https://fl-web-boilerplate-uat.vercel.app && npx cypress open",
    "cypress:open:qa": "set CYPRESS_BASE_URL=https://fl-web-boilerplate-uat.vercel.app && npx cypress open --env version=qa",
    "chromatic": "chromatic --exit-zero-on-changes",
    "storybook": "storybook dev -p 6006",
    "svg-to-react": "svgr assets/icons --out-dir common/components/icons --icon --typescript --prettier-config .prettierrc",
    "generate-scss-theme": "json-to-scss common/theme/theme.json",
    "create-theme": "bash scripts/designTokensGenerator/createTheme.sh",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@hookform/resolvers": "^3.1.0",
    "@stripe/stripe-js": "^2.1.9",
    "@types/node": "20.0.0",
    "@types/react-dom": "18.2.3",
    "@vercel/edge-config": "^0.2.1",
    "autoprefixer": "10.4.14",
    "dayjs": "^1.11.9",
    "eslint-config-next": "13.5.6",
    "mixpanel-browser": "^2.47.0",
    "next": "^13.5.6",
    "next-auth": "^4.22.1",
    "postcss": "^8.4.31",
    "react": "18.2.0",
    "react-avatar-editor": "^13.0.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.44.3",
    "react-hot-toast": "^2.4.1",
    "sass": "^1.63.3",
    "stripe": "^12.10.0",
    "swr": "^2.1.5",
    "tailwindcss": "3.3.2",
    "typescript": "^5.0.4",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@cypress-audit/lighthouse": "^1.4.2",
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.12",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/blocks": "^7.0.12",
    "@storybook/nextjs": "^7.0.12",
    "@storybook/react": "^7.0.12",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@svgr/cli": "^8.0.1",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/mixpanel-browser": "^2.38.1",
    "@types/react": "^18.2.5",
    "@types/react-avatar-editor": "^13.0.0",
    "@types/whatwg-fetch": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "chromatic": "^6.17.4",
    "cypress": "^13.3.3",
    "cypress-audit": "^1.0.0",
    "cypress-plugin-stripe-elements": "^1.0.2",
    "cypress-slow-down": "^1.2.1",
    "eslint": "^8.48.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "json-to-scss": "^1.6.2",
    "msw": "^2.0.6",
    "storybook": "^7.0.12",
    "ts-node": "^10.9.1",
    "undici": "^5.27.2",
    "whatwg-fetch": "^3.6.19"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
